{
  "name": "boot-express",
  "version": "1.2.0",
  "description": "Bootstraps express ",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run dev",
    "lint": "jshint *.js lib/*.js lib/**/*.js --reporter=node_modules/jshint-stylish || true",
    "dev": "nodemon"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules/*",
      ".hg"
    ],
    "watch": [
      "lib/*",
      "*.js"
    ],
    "ext": "js json",
    "exec": "npm run lint  && node test/test.js "
  },
  "keywords": [
    "express",
    "bootstrap",
    "application"
  ],
  "author": "itzco",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "debug-env": "^1.1.2",
    "express": "^4.16.3",
    "express-pino-logger": "^3.0.2",
    "fs-extra": "^7.0.0",
    "helmet": "^3.13.0",
    "mysql": "^2.16.0",
    "rethinkdb": "^2.3.3",
    "stack-trace": "0.0.10"
  },
  "devDependencies": {
    "jshint": "^2.9.5",
    "jshint-stylish": "^2.2.1",
    "nodemon": "^1.18.3"
  }
}
